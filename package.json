{
  "name": "sonarqube-ai",
  "displayName": "SonarQube AI",
  "description": "AI-powered SonarQube issue analysis and automated bug fixing for VS Code",
  "version": "0.0.1",
  "publisher": "MajesticRupesh",
  "author": "Rupesh Pal",
  "license": "MIT",
  "homepage": "https://github.com/MajesticRupesh/sonarqube-ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/MajesticRupesh/sonarqube-ai.git"
  },
  "bugs": {
    "url": "https://github.com/MajesticRupesh/sonarqube-ai/issues"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Linters",
    "Other"
  ],
  "keywords": [
    "sonarqube",
    "ai",
    "code-quality",
    "static-analysis",
    "bug-fixing",
    "automation"
  ],
  "activationEvents": [
    "onCommand:sonarqube-ai.getIssues",
    "onCommand:sonarqube-ai.fixBugs"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sonarqube-ai.getIssues",
        "title": "Get SonarQube Issues"
      },
      {
        "command": "sonarqube-ai.fixBugs",
        "title": "Fix SonarQube Bugs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  }
}
